{
  "name": "vitepress-plugin-codesandbox",
  "version": "0.1.1",
  "description": "Add 'Open in CodeSandbox' buttons to VitePress code blocks",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "vitepress",
    "vitepress-plugin",
    "codesandbox",
    "code-blocks",
    "documentation"
  ],
  "author": "humanbydefinition",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/humanbydefinition/vitepress-plugin-codesandbox.git"
  },
  "bugs": {
    "url": "https://github.com/humanbydefinition/vitepress-plugin-codesandbox/issues"
  },
  "homepage": "https://github.com/humanbydefinition/vitepress-plugin-codesandbox#readme",
  "peerDependencies": {
    "vitepress": ">=1.0.0",
    "vue": ">=3.3.0"
  },
  "dependencies": {
    "lz-string": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitepress": "^1.5.0",
    "vue": "^3.5.13"
  }
}
